alias g="git"
alias s="git status "
alias a="git add "
alias ap="git app -p "
alias d="git diff "
alias c="git commit -S -m "
alias ca="git commit -S --amend "
alias b="git checkout "
alias r="git reset "
alias ch="git checkout "
alias push="git push "
alias pull="git pull "
alias f="git fetch "

function tag {
    git tag -s $1 -m $1
}

